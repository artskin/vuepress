{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "module": "es2015",
    "newLine": "lf",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2015",
    "include": [
      "docs/**/*.ts",
      "docs/**/*.tsx",
      "docs/**/*.vue",
      "docs/**/*.ts",
      "docs/**/*.md"
    ],
  }
}
